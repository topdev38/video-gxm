{
  "name": "gxm-video-web",
  "version": "1.0.0",
  "description": "GXM Video Web",
  "author": "WJ",
  "private": true,
  "scripts": {
    "jslint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "csslint": "stylelint '**/*.vue' '**/*.css' '**/*.scss' '!coverage' '!dist' --config stylelint.config.js",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "npm run jslint && npm run csslint",
    "lint-fix": "npm run jslint -- --fix && npm run csslint -- --fix",
    "test": "jest",
    "e2e:open": "cypress open",
    "heroku-postbuild": "npm run build"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/google-tag-manager": "^2.3.2",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@videojs/http-streaming": "^1.13.3",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.4.0",
    "cross-env": "^5.2.0",
    "currency-symbol-map": "^4.0.4",
    "express": "^4.16.4",
    "moment": "^2.27.0",
    "nuxt": "^2.13.3",
    "nuxt-facebook-pixel-module": "^1.3.0",
    "nuxt-helmet": "^1.2.3",
    "redirect-ssl": "^1.4.1",
    "request": "^2.88.2",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^7.0.0",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.18.0",
    "throng": "^4.0.0",
    "uuid": "^3.4.0",
    "videojs-contrib-hls": "^5.15.0",
    "vue-cookie-law": "1.12.0",
    "vue-notification": "^1.3.20",
    "vue-script2": "^2.1.0",
    "vue-video-player": "^5.0.2",
    "vue2-touch-events": "^2.2.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.2.0",
    "@nuxtjs/stylelint-module": "^3.2.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.1.0",
    "cypress": "^3.8.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^2.6.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.14.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "sass-loader": "^8.0.2",
    "stylelint": "^10.1.0",
    "typescript": "^3.9.7",
    "vue-jest": "^4.0.0-beta.4"
  }
}
